            a {
                text-decoration: none;
            }
        }
    </style>
</head>

<script>
    function validateform() {
        var name = document.forms["myform"]["name"].value;
        var mail = document.forms["myform"]["mail"].value;
        var organization = document.forms["myform"]["organization"].value;
        var mobile = document.forms["myform"]["mobile"].value;
        var message = document.forms["myform"]["message"].value;

        if (name == "" || name == null) {
            alert("Name Field cannot be empty!!");
            return false;
        }
        else if (mail == "" || mail == null) {
            alert("Mail Field cannot be empty!!");
            return false;
        }
        else if (organization == "" || organization == null) {
            alert("Organization Field cannot be empty!!");
            return false;
        }
        else if (mobile.length <= 0) {
            alert("Enter Mobile Number!!")
            return false;
        }
        else if (mobile.length != 10) {
            alert("Entered Mobile Number should be 10 Digit Long!!");
            return false;
        }
        else if (message == "" || message == null) {
            alert("Please type your message!!");
            return false;
        }
        else {
            return true;
        }
    }
</script>

